<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteRule ^storage/(.*)$ public/storage/$1 [L]
    RewriteRule ^media/(.*)$ /storage/media/$1 [L]
    RewriteRule ^logo/(.*)$ /storage/logo/$1 [L]
    RewriteRule ^favicon/(.*)$ /storage/favicon/$1 [L]
    RewriteRule ^pages/(.*)$ /storage/pages/$1 [L]
    RewriteRule ^avatars/(.*)$ /storage/avatars/$1 [L]
    RewriteRule ^business_assets/(.*)$ /storage/business_assets/$1 [L]
    RewriteRule ^explore_sliders/(.*)$ /storage/explore_sliders/$1 [L]
    RewriteRule ^badges/(.*)$ /storage/badges/$1 [L]
    
    RewriteCond %{REQUEST_URI} !^/public/
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

<IfModule mime_module>
    AddHandler application/x-httpd-ea-php82 .php .php8 .phtml
</IfModule>

